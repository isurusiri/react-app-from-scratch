{
  "name": "react-app-from-scratch",
  "version": "1.0.0",
  "description": "An example showing how to build a react app from the scratch.",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,jsx,css,json}\" ",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,css,json}\" ",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "lint:debug": "npm run lint -- --debug",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "parcel src/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isurusiri/react-app-from-scratch.git"
  },
  "keywords": [],
  "author": "Isuru Siriwardana",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/isurusiri/react-app-from-scratch/issues"
  },
  "homepage": "https://github.com/isurusiri/react-app-from-scratch#readme",
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.15.3"
  },
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  }
}
